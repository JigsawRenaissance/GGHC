proc vercmp {ver1 ver2} {
foreach v1 [split $ver1 ._-] v2 [split $ver2 ._-] {
if {$v1 eq ""} { set v1 0 }
if {$v2 eq ""} { set v2 0 }
if {$v1 < $v2} { return -1 }
if {$v1 > $v2} { return 1 }
}
return 0
}

